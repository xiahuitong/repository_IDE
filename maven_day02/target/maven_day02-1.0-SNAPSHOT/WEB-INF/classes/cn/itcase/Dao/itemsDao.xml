<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itcase.Dao.itemsDao">
    <!--resultMap 返回值  parameterType 参数类型-->
    <select id="sava" resultMap="Items" parameterType="int">
        select * FROM  Items WHERE id=#{id}
    </select>
</mapper>